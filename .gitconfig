[alias]
  co = checkout
  b  = branch
  s  = status
[user]
  name = Jonathan Suh
  email = hello@jonsuh.com
[github]
  user = jonsuh
  token =
[core]
  editor = s -w
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[color "branch"]
  current = white green bold
  local = yellow
  remote = yellow reverse
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = cyan
[http]
  postBuffer = 524288000
[push]
  default = current

[filter "media"]
  required = true
  clean = git media clean %f
  smudge = git media smudge %f
[filter "hawser"]
  clean = git hawser clean %f
  smudge = git hawser smudge %f
  required = true
[filter "lfs"]
  clean = git lfs clean %f
  smudge = git lfs smudge %f
  required = true
